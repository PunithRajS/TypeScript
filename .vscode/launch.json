{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Playwright Tests",
        "type": "node",
        "request": "launch",
        "program": "${workspaceFolder}/node_modules/.bin/playwright",
        "args": ["test", "--debug"],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "cwd": "${workspaceFolder}"
      }
    ]
  }
  